using System; 
   class Program { 
 public static void Main (string[] args) { 
  int sum = 0;
   int n = 0;
   do {
     n++;
     sum += n;
   } while (n < 50);
   Console.WriteLine(sum);

   int summ = 0;
   int a = -1;
   do {
     a += 2;
     summ += a;
   } while (a < 49);
   Console.WriteLine(summ);

   int summm = 0;
   int b = 26;
   do {
     b++;
     summm += b;
   } while (b < 76);
   Console.WriteLine(summm);
  }
}
