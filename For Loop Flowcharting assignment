using System; 
   class Program { 
 public static void Main (string[] args) { 
  int sum = 0;
   for (int n = 0; n < 51; n++) {
     sum += n;
   }
   Console.WriteLine(sum);
     
   int sama = 0;
   for (int n = -1; n < 51; n += 2) {
     sama += n;
   }
     Console.WriteLine(sama);

    int summ = 0;
   for (int n = 24; n < 76; n++) {
     summ += n;
   }
     Console.WriteLine(summ);
  }
}
