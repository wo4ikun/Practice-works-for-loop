using System; 
   class Program { 
 public static void Main (string[] args) {
    int [] num = new int [8];
     int sum = 0;
   Console.WriteLine("Enter the array elements");
   for (int i = 0; i < 8; i++) {
     num [i] = Convert.ToInt32(Console.ReadLine());
      sum += num[i];
   }
   Console.WriteLine("Arrays elements are");
  for (int i = 0; i < 8; i++) {
    Console.WriteLine(i);
  }
   Console.WriteLine("Sum is {0}", sum);
  }
}
